                  <Button maxHeight="1.0" maxWidth="10.0" minHeight="1.0" minWidth="10.0" onMousePressed="#windowMinimize" mnemonicParsing="false" prefHeight="1.0" prefWidth="10.0" />
               </children>
            </StackPane>
            <StackPane maxHeight="35.0" maxWidth="45.0" minHeight="35.0" minWidth="45.0" onMousePressed="#windowClose" prefHeight="35.0" prefWidth="45.0" styleClass="close-btn">
               <children>
                  <Button maxHeight="10.0" maxWidth="10.0" minHeight="10.0" minWidth="10.0" onMousePressed="#windowClose" mnemonicParsing="false" prefHeight="10.0" prefWidth="10.0" />
               </children>
            </StackPane>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
